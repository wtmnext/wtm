[
  {
    "pattern": "^$|^\\/$|^\\/assets\\/.*$|^favicon.ico$",
    "unauthenticated": false,
    "authenticated": false
  },
  {
    "pattern": "^/users/(new|activate|login)$",
    "unauthenticated": true,
    "authenticated": false
  },
  {
    "pattern": "^/admin/?.*",
    "unauthenticated": false,
    "authenticated": true,
    "roles": [
      "ADMIN"
    ]
  },
  {
    "pattern": "^/organizations/?.*",
    "unauthenticated": false,
    "authenticated": true,
    "roles": [
      "SUPERADMIN"
    ]
  },
  {
    "pattern": "^/users/logout$",
    "authenticated": true,
    "unauthenticated": false
  }
]
